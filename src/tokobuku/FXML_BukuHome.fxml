      <ImageView fitHeight="700.0" fitWidth="1000.0" pickOnBounds="true">
         <image>
            <Image url="@../Images/BookStore.png" />
         </image>
      </ImageView>
      <Button fx:id="btnMasuk" alignment="CENTER" layoutX="414.0" layoutY="574.0" mnemonicParsing="false" onAction="#masukKlik" prefHeight="81.0" prefWidth="229.0" style="-fx-background-radius: 5em;" text="Masuk" textAlignment="CENTER" textFill="BLUE">
         <font>
            <Font name="Gill Sans Ultra Bold Condensed" size="36.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btnKembali" alignment="CENTER" layoutX="24.0" layoutY="26.0" mnemonicParsing="false" onAction="#kembaliKlik" prefHeight="42.0" prefWidth="131.0" style="-fx-background-radius: 1em;" text="Kembali" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Gill Sans Ultra Bold Condensed" size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="108.0" layoutY="72.0" prefHeight="71.0" prefWidth="785.0" style="-fx-alignment: center;" text="Bright's Store" textAlignment="CENTER">
         <font>
            <Font name="Lucida Calligraphy Italic" size="67.0" />
         </font>
         <effect>
            <Glow level="1.0">
               <input>
                  <DropShadow offsetX="2.0" offsetY="2.0" radius="12.2225" spread="1.0" width="29.89" />
               </input>
            </Glow>
         </effect>
         <textFill>
            <LinearGradient endX="1.0" endY="1.0" startX="0.004761904761904745" startY="0.34285714285714286">
               <stops>
                  <Stop color="#00ffdd" />
                  <Stop color="#00ffdd" offset="0.007662835249042145" />
                  <Stop color="#59c700" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Label>
   </children>
</AnchorPane>
