<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sistempembelianpenjualan.FXML_MenuController">
   <children>
      <ImageView fitHeight="369.0" fitWidth="600.0" layoutY="32.0" pickOnBounds="true">
         <image>
            <Image url="@../Images/background1.png" />
         </image>
      </ImageView>
      <MenuBar prefHeight="32.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="Input">
            <items>
                  <MenuItem fx:id="miInputBarang" mnemonicParsing="false" onAction="#inputBarangKlik" text="Input Barang" />
                  <MenuItem fx:id="miInputBuku" mnemonicParsing="false" onAction="#inputBukuKlik" text="Input Buku" />
                  <MenuItem fx:id="miInputElektronik" mnemonicParsing="false" onAction="#inputElektronikKlik" text="Input Elektronik" />
            </items>
          </Menu>
            <Menu fx:id="dataCust" mnemonicParsing="false" text="Data">
              <items>
                  <MenuItem fx:id="barangCustomer" mnemonicParsing="false" onAction="#dataBarangKlik" text="Data Barang" />
                  <MenuItem fx:id="miDetilPenjualan" mnemonicParsing="false" onAction="#detilPenjualanKlik" text="Detil Penjualan" />
              </items>
            </Menu>
          <Menu fx:id="toko" mnemonicParsing="false" text="Toko">
            <items>
                  <MenuItem fx:id="tokoElektronik" mnemonicParsing="false" onAction="#tokoElektronikKlik" text="Toko Elektronik" />
                  <MenuItem fx:id="tokoBuku" mnemonicParsing="false" onAction="#tokoBukuKlik" text="Toko Buku" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="logoutButton" layoutX="463.0" layoutY="319.0" mnemonicParsing="false" onAction="#logoutKlik" prefHeight="57.0" prefWidth="116.0" style="-fx-background-radius: 5em;" text="Logout">
         <font>
            <Font name="Book Antiqua Bold" size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="belanjaKeTokoLain" layoutX="26.0" layoutY="319.0" mnemonicParsing="false" onAction="#belanjaKeTokoLainKlik" prefHeight="57.0" prefWidth="305.0" style="-fx-background-radius: 5em;" text="Belanja Ke Toko Lain">
         <font>
            <Font name="Gill Sans Ultra Bold Condensed" size="25.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
