      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#9aa2ab" height="819.0" layoutX="370.0" layoutY="65.0" stroke="BLACK" strokeType="INSIDE" width="592.0" />
      <Label fx:id="gradient1" layoutX="517.0" layoutY="73.0" prefHeight="96.0" prefWidth="303.0" text="Register" textAlignment="CENTER" textFill="WHITE">
