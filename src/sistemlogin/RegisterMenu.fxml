<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sistemlogin.RegisterMenuController">
   <children>
      <ImageView fitHeight="938.0" fitWidth="1310.0" focusTraversable="true" pickOnBounds="true">
         <image>
            <Image url="@../Images/background.jpg" />
