      </PasswordField>
      <TextField fx:id="txtNoTelepon" focusTraversable="false" layoutX="414.0" layoutY="400.0" prefHeight="69.0" prefWidth="509.0" promptText="No Telepon" style="-fx-background-radius: 7em;">
