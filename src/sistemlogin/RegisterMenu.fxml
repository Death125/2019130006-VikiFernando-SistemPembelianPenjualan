            <Font name="Geometr415 Blk BT Black" size="79.0" />
         </font>
      </Label>
      <TextField fx:id="txtEmail" focusTraversable="false" layoutX="415.0" layoutY="304.0" prefHeight="69.0" prefWidth="501.0" promptText="Email" style="-fx-background-radius: 7em;">
         <font>
            <Font name="Franklin Gothic Demi Cond" size="30.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" focusTraversable="false" layoutX="414.0" layoutY="502.0" prefHeight="69.0" prefWidth="509.0" promptText="Password" style="-fx-background-radius: 7em;">
