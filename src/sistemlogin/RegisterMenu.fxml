         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btnBack" layoutX="428.0" layoutY="793.0" mnemonicParsing="false" onAction="#backKlik" prefHeight="68.0" prefWidth="479.0" style="-fx-background-radius: 5em;" text="Back">
         <font>
            <Font size="31.0" />
